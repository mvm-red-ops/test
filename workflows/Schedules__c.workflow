        <formula>AND( ISPICKVAL(Deal_Parent__r.Program__r.Cable_Synd__c , &quot;Cable&quot;),  if(TEST__c =True,true, false),  Units_Aired__c &gt;0, NOT(ISPICKVAL(Status__c, &quot;Canceled&quot;)))</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Conga_Cable_Test_Follow_Up</name>
                <type>OutboundMessage</type>
            </actions>
            <offsetFromField>Schedules__c.Week__c</offsetFromField>
            <timeLength>9</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
