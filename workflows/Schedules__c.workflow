            <type>Alert</type>
        </actions>
        <active>false</active>
        <description>Sends email to Jake, Sabry and Diane everytime the value of the Traffic Error Cost  field is edited.</description>
        <formula>ISCHANGED(Error_Cost__c)</formula>
        <triggerType>onAllChanges</triggerType>
