    <rules>
        <fullName>Unghost Schedule if using Partially Ghosted</fullName>
        <actions>
            <name>Unghost_Schedule_if_using_Partially_Ghos</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <formula>AND(OR( TEXT(Cap__c) =&quot;50%&quot;,TEXT(Cap__c)=&quot;80%&quot;), Ghost_Order__c = True)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Unphantomed</fullName>
        <actions>
            <name>Unphantom</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND(NOT(ISPICKVAL(Cap__c,&quot;GHOST&quot;)),Phantom__c=TRUE,ISCHANGED(Cap__c))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Update %240 Bundle Schedules</fullName>
        <actions>
            <name>Units_Available_1</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Effective 8/31/15 if a bundle schedule has 0 units aired and $0 rate but spot count is greater than 0, update the units aired to 1. This allows finance to invoice the schedule. It affects both bonus schedules and spots that are running after cancellation.</description>
        <formula>if( 
 AND( Week__c  &gt;= date(2015,8,31),     
      Deal_Parent__r.Program__r.Bundle__c =True, 
      Units_Available__c = 0,  
      Rate__c = 0, 
      Spot_Count__c &gt;0
     )
,TRUE,FALSE)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Update Previous Units Available</fullName>
        <actions>
            <name>Update_Units_Previous_Available</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>ISCHANGED(Units_Available__c) &amp;&amp;  DealBrand__c =&quot;Availability&quot; &amp;&amp;  ISPICKVAL(Deal_Parent__r.Program__r.Cable_Synd__c,&quot;Syndication&quot;)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
</Workflow>
