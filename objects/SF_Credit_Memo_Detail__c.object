    <validationRules>
        <fullName>SFCM_Complete</fullName>
        <active>true</active>
        <description>This rule will ensure that no changes are made to any SFCM that has been processed and is marked as &quot;complete&quot;.</description>
        <errorConditionFormula>SF_Credit_Memo_Detail__r.Complete__c =TRUE</errorConditionFormula>
        <errorMessage>This SFCM has been processed and marked off as complete by Finance. Please consult with Finance before making any changes. All changes must be approved by Finance.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SF_CM_Schedules_equal_Master_Deal</fullName>
        <active>true</active>
        <description>This validation rule will confirm that the schedules selected belong to the master deal</description>
        <errorConditionFormula>IF( Schedule__r.Deal_Parent__c &lt;&gt;  SF_Credit_Memo_Detail__r.Deal__c  ,true, False)</errorConditionFormula>
        <errorMessage>Schedule selected does not appear on the list of schedules found on the deal.</errorMessage>
    </validationRules>
