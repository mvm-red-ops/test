    <webLinks>
        <fullName>Start_Import_Process</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Start Import Process</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/38.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/38.0/apex.js&quot;)}

if(confirm(&apos;Begin import process?&apos;)) {

var callbacks = { 
onSuccess: function(result) {
alert(&apos;Import process started successfully.&apos;);
}, 
onFailure: function(error) {
alert(&apos;An unexpected error occurred: &apos; + error);
}
};

var result = sforce.apex.execute(
&apos;RaycomMonthlyLineItemBatch&apos;,
&apos;init&apos;,
{},
callbacks
);

}</url>
    </webLinks>
</CustomObject>
