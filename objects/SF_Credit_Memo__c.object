    <validationRules>
        <fullName>At_Fault_Review</fullName>
        <active>true</active>
        <description>If SFCM type equals &apos;Credit&apos;, record must identify who caused the error (MV, Network)</description>
        <errorConditionFormula>AND(OR( ISPICKVAL( Type__c , &quot;Credit - Approved by Network&quot;), ISPICKVAL(Type__c, &quot;Credit&quot;)), ISPICKVAL( Error_Caused_By__c , &quot;&quot;))</errorConditionFormula>
        <errorMessage>If SFCM Type = Credit, Please populate the &apos;Error Caused By&apos; field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Double_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNEW(),ISBLANK(Double_Check__c))</errorConditionFormula>
        <errorMessage>Please populate Double Check field with appropriate MVMS Team Member.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_Enter_Comments</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Comments__c )</errorConditionFormula>
        <errorDisplayField>Comments__c</errorDisplayField>
        <errorMessage>Please add an explanation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SFCM_Complete</fullName>
        <active>false</active>
        <description>This rule will ensure that no changes are made to any SFCM that has been processed and is considered &quot;complete&quot;.</description>
        <errorConditionFormula>Complete__c = True</errorConditionFormula>
        <errorMessage>This SFCM has been processed and marked off as complete by Finance. Please consult with Finance before making any changes. All changes must be approved by Finance.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
