        <errorConditionFormula>ISPICKVAL( Type__c , &quot;&quot;)</errorConditionFormula>
        <errorMessage>Please update Asset Description &quot;TYPE&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Call_Forwarded</fullName>
        <active>false</active>
        <errorConditionFormula>AND( Call_Forwarded__c = TRUE,ISBLANK(When_Call_Forwarded__c))</errorConditionFormula>
        <errorMessage>Please make sure to update when calls were forwarded</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Requires_Phone_Number</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Type__c , &quot;Polycom Phone&quot;), ISBLANK(Phone_Number__c))</errorConditionFormula>
        <errorDisplayField>Phone_Number__c</errorDisplayField>
        <errorMessage>Requires Phone Number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Requires_Serial</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR(ISPICKVAL(Type__c , &quot;iMac&quot;),ISPICKVAL(Type__c , &quot;iPad&quot;),ISPICKVAL(Type__c , &quot;Macbook&quot;),ISPICKVAL(Type__c , &quot;Macbook Air&quot;),ISPICKVAL(Type__c , &quot;Macbook Pro&quot;),ISPICKVAL(Type__c , &quot;Mac Mini&quot;),ISPICKVAL(Type__c , &quot;Polycom Phone&quot;)), ISBLANK(Serial__c))</errorConditionFormula>
        <errorDisplayField>Serial__c</errorDisplayField>
        <errorMessage>Requires Mac Serial or Phone ID</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
