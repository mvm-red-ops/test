    <validationRules>
        <fullName>AOR_Review_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Application_Status__c ,&quot;Pending Finance Manager Review&quot;),ISPICKVAL( Application_Type__c , &quot;New Advertiser&quot;), AOR_Received__c = FALSE )</errorConditionFormula>
        <errorMessage>Finance must obtain an AOR. The AOR must be reviewed and authorized by Finance Manager before credit application can be approved and completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Finance_Credit_Term_Support_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Application_Status__c ,&quot;Pending Finance Manager Review&quot;),ISBLANK(Finance_Credit_Term_Support__c))</errorConditionFormula>
        <errorMessage>Finance Manager must provide comments supporting credit limit/ terms granted</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
